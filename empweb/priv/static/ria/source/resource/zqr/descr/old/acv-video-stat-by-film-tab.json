{
    "type" : "table",
    "toolbar" : [
        {"type" : "back"},
        
        {"type" : "button",
            "name" : "Обновить",
            "action" : "/get-all-acv-video-stats",
            "icon" : "icon/32/actions/view-refresh.png",
            "specParam": "tab-change-state-action"}
    ],
    "columns" : [
        {"name" : "id",
            "alias" : "#",
            "type" : "float",
            "sortable" : true,
            "width":"5%"
        },
        {"name" : "peer",
            "alias" : "ip",
            "type" : "string",
            "sortable" : true
        },
        {"name" : "datestart",
            "alias" : "Время начала",
            "type" : "erl_datetime",
            "sortable" : true
        },
        {"name" : "datestop",
            "alias" : "Время конца",
            "type" : "erl_datetime",
            "sortable" : true
        },
        {"name" : "click",
            "alias" : "Дата клика",
            "type" : "erl_datetime_nullable",
            "sortable" : true
        }
    ],

    "filter" : {
        "fields" : [
            {"name" : "fromdate",
                "alias" : "с даты",
                "input" : {
                    "type" : "date-field-md"
                }
            },
            {"name" : "todate",
                "alias" : "по дату",
                "input" : {
                    "type" : "date-field-pd"
                }
            }
        ],
        "render" : "horisontal",
        "submit_url" : "/get-acv-video-stat/by-film"
    },

    "index_name" : ["video_url", "parent_id"],
    "sort" : "id",
    "ascending" : false,

    // "dblclick_action" : "resource/zqr/descr/acv-video-stat-by-film-tab.json"
}
